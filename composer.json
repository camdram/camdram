{
	"name" : "camdram/camdram",
	"description" : "Camdram - The portal website for student theatre in Cambridge",
	"autoload" : {
		"psr-4" : {
			"" : "src/"
		},
		"classmap" : [
			"app/AppKernel.php",
			"app/AppCache.php"
		]
	},
	"autoload-dev": {
		"psr-4": {
			"Camdram\\Tests\\": "tests/"
		}
	},
	"license" : "GPL-2.0",
	"require" : {
		"php" : ">=7.0.0",
		"symfony/symfony" : "3.4.*",
		"doctrine/orm" : "^2.5",
		"doctrine/doctrine-bundle" : "^1.9",
		"doctrine/doctrine-migrations-bundle" : "1.3.*",
		"doctrine/doctrine-fixtures-bundle" : "~3.0",
		"stof/doctrine-extensions-bundle" : "1.3.*",
		"twig/twig" : "^1.0||^2.0",
		"twig/extensions" : "~1.5",
		"symfony/swiftmailer-bundle" : "^2.6.4",
		"symfony/monolog-bundle" : "^3.3",
		"sensio/distribution-bundle" : "^5.0",
		"sensio/framework-extra-bundle" : "^5.2",
		"friendsofsymfony/rest-bundle" : "^2.3",
		"jms/serializer-bundle" : "^2.4",
		"friendsofsymfony/oauth-server-bundle" : "^1.6",
		"egeloen/google-map-bundle" : "^3.0",
		"pagerfanta/pagerfanta" : "1.0.*",
		"friendsofsymfony/jsrouting-bundle" : "2.2.*",
		"sabre/vobject" : "~4.1",
		"friendsofsymfony/elastica-bundle": "^5.0",
        "incenteev/composer-parameter-handler" : "^2.1",
		"liip/imagine-bundle": "~1.9",
		"zurb/foundation" : "4.*",
        "zendframework/zend-feed": "2.4.*",
        "zendframework/zend-servicemanager": "2.4.*",
        "excelwebzone/recaptcha-bundle" : "^1.5",
        "hwi/oauth-bundle": "^0.6",
        "oneup/uploader-bundle": "^1.9",
        "facebook/graph-sdk": "^5.6",
        "abraham/twitteroauth": "^0.9",
        "knplabs/github-api": "^2.8",
        "php-http/guzzle6-adapter": "^1.1",
        "php-http/httplug-bundle": "^1.10"
	},
	"require-dev" : {
		"behat/behat": "3.4.*",
		"behat/mink": "^1.7",
		"behat/mink-extension": "^2.3",
		"behat/mink-goutte-driver": "*",
		"behat/symfony2-extension": "^2.1",
		"dama/doctrine-test-bundle": "^4.0",
		"sensio/generator-bundle": "^3.1",
		"symfony/phpunit-bridge": "^4.1",
		"symfony/web-server-bundle": "^3.4"
	},
	"scripts" : {
		"post-create-project-cmd": [
            "Acts\\CamdramAdminBundle\\Composer\\ScriptHandler::refreshDatabase",
            "Acts\\CamdramAdminBundle\\Composer\\ScriptHandler::welcomeMessage"
        ],
		 "symfony-scripts": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget"
        ],
        "post-install-cmd": [
            "@symfony-scripts"
        ],
        "post-update-cmd": [
            "@symfony-scripts"
        ]
	},
	"minimum-stability" : "stable",
	"extra" : {
		"symfony-app-dir" : "app",
		"symfony-web-dir" : "web",
        "symfony-bin-dir": "bin",
        "symfony-var-dir": "var",
        "symfony-tests-dir": "tests",
		"incenteev-parameters" : {
			"file" : "app/config/parameters.yml"
		},
		"symfony-assets-install" : "relative"
	},
	"config" : {
		"bin-dir" : "bin/",
		"platform": {
            "php": "7.0.30"
        },
        "sort-packages": true
	}
}
