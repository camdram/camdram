<!DOCTYPE html>
<!--[if lt IE 7]>      <html class="no-js lt-ie9 lt-ie8 lt-ie7"> <![endif]-->
<!--[if IE 7]>         <html class="no-js lt-ie9 lt-ie8"> <![endif]-->
<!--[if IE 8]>         <html class="no-js lt-ie9"> <![endif]-->
<!--[if gt IE 8]><!--> <html class="no-js"> <!--<![endif]-->
<head>
    <head>
        <meta charset="utf-8" />
        <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">

        <meta name="keywords" content="cambridge,theatre,drama,comedy,shows,society,societies,ACTS,association" />
        <meta name="description" content="ACTS: Association of Cambridge Theatre Societies. Our website contains details of forthcoming shows and miscellaneous information on Cambridge Drama." />

        <!-- Set the viewport width to device width for mobile -->
        <meta name="viewport" content="width=device-width" />
        <link rel='icon' href='/favicon.png' type='image/png' />

        <title>{% block title %}{% endblock %} - Camdram</title>


        <link href="//netdna.bootstrapcdn.com/font-awesome/4.0.3/css/font-awesome.css" rel="stylesheet">
        {% stylesheets output='css/main.css'
        'bundles/actscamdram/stylesheets/app/*.scss'
        'bundles/actsdiary/css/diary.css'
        'bundles/actscamdram/stylesheets/jquery/*.css'
        'bundles/hoyesimagemanager/css/fancybox.css'%}
        <link rel="stylesheet" href="{{ asset_url }}" />
        {% endstylesheets %}

        {% block stylesheets %}{% endblock %}

        <script src="//ajax.googleapis.com/ajax/libs/jquery/1.8.3/jquery.min.js"></script>
        <script src="//ajax.googleapis.com/ajax/libs/jqueryui/1.9.2/jquery-ui.min.js"></script>
        {% javascripts output='js/deps.js'
        '@ActsCamdramBundle/Resources/public/javascripts/jquery/jquery-ui.touchpunch.js'
        '@ActsCamdramBundle/Resources/public/javascripts/foundation/custom.modernizr.js'
        '@ActsCamdramBundle/Resources/public/javascripts/foundation/foundation.min.js'
        '@ActsCamdramBundle/Resources/public/javascripts/moment.js'
        '@ActsDiaryBundle/Resources/public/js/diary.js'
        '@FOSJsRoutingBundle/Resources/public/js/router.js'
        '@HoyesImageManagerBundle/Resources/public/js/jquery.uploadify.js'
        '@HoyesImageManagerBundle/Resources/public/js/jquery.fancybox.js' %}
        <script type="text/javascript" src="{{ asset_url }}"></script>
        {% endjavascripts %}
        {% javascripts vars=['env'] output='js/main.{env}.js'
        'js/fos_js_routes.js'
        '@ActsCamdramBundle/Resources/public/javascripts/app/camdram.js'
        '@ActsCamdramBundle/Resources/public/javascripts/app/autocomplete.js'
        '@ActsCamdramBundle/Resources/public/javascripts/app/home.js'%}
        <script type="text/javascript" src="{{ asset_url }}"></script>
        {% endjavascripts %}
        {% block javascripts %}{% endblock %}
    </head>
    <body>
        {% block main %}{% endblock %}


        <footer>
            {% block footer %}{% endblock %}
            <div id="copyright" class="row">
                <p>&copy; Association of Cambridge Theatre Societies and contributing groups 2004 &#8212; {{ "now"|date('Y') }}.
                    Comments &amp; queries to <a href="mailto:websupport@camdram.net">websupport@camdram.net</a>
                </p>
            </div>
        </footer>

        <script>
          (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
          (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
          m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
          })(window,document,'script','//www.google-analytics.com/analytics.js','ga');

          ga('create', 'UA-37770738-2', 'camdram.net');
          ga('send', 'pageview');
        
        </script>
    </body>
</html>
